programa {
  funcao inicio() {
    inteiro n1, n2, w, x, y, z
    escreva("Digite o primeiro número: ")
    leia(n1)
    escreva("Digite o segundo número: ")
    leia(n2)
    w = n1 + n2
    x = n1 - n2
    y = n1 * n2
    z = n1 / n2
    escreva("W: ",w, "\n")
    escreva("X: ",x, "\n")
    escreva("Y: ",y, "\n")
    escreva("Z: ",z, "\n")
  }
}
